  jnz col5_0
  mov dx,bx
  sub dx,26
  push ax
  push bx
  push cx
  mov cx,temp_find_?_columns
  call colour_nodes_even_column_zeros
  pop cx
  pop bx
  pop ax
  jmp repeat_0
  col5_0:
