  push ax
  push bx
  push cx
  mov cx,temp_find_?_columns
  call colour_nodes_even_column_undefined
  pop cx
  pop bx
  pop ax
  jmp repeat_5
  col7_5:
  cmp temp_find_?_columns,7
  jnz col8_5
