  push ax
  push bx
  push cx
  mov cx,temp_find_?_columns
  call colour_nodes_odd_column_zeros
  pop cx
  pop bx
  pop ax
  jmp repeat_0
  col6_0:
  cmp temp_find_?_columns,6
  jnz col7_0
