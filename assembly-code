  push ax
  push bx
  push cx
  mov cx,temp_find_?_columns
  call colour_nodes_odd_column_undefined
  pop cx
  pop bx
  pop ax
  jmp repeat_5
  col6_5:
  cmp temp_find_?_columns,6
  jnz col7_5
  mov dx,bx
