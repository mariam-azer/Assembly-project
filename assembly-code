    cmp gate_number,31h    ;letter n for xnor has scancode 31h
    jnz check_not_op2
    mov bx,first_row_leg
    sub bx,1
    mov al,arr_col2[bx]
    cmp al,5
    jz undefined6_col2
;;not_undefined:
    mov temp_gates,al
    mov bx,second_row_leg
    sub bx,1
    mov al,arr_col2[bx]
    cmp al,5
